public integer rand()
public integer atoi(string s)
public void printr(integer i)
public void println()

public integer entry (integer argc, string *argv, string *envp) {
	integer n   := atoi(argv[1]);
	integer sum := 0;
	integer i   := 0;
	number res  := 0;

	for i in 1 upto n do sum := sum + rand();
	res := sum / n;
	printr(res);
	println()

	entry := 0
};
